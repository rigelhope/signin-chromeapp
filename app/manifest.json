{
    "name": "bikefarm sign-in",
    "description": "",
    "version": "10",
    "manifest_version": 2,
    "default_locale": "en",
    "permissions": [
        "storage",
        "app.runtime",
        {
            "fileSystem": [
                "write",
                "retainEntries",
                "directory"
            ]
        },
        "<all_urls>"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "scripts/main.js",
                "scripts/chromereload.js"
            ]
        }
    }
}